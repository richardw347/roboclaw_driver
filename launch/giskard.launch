<launch>
   <node respawn="true" name="roboclaw_driver" pkg="roboclaw_driver" type="roboclaw_driver" output="screen">
		<param name="port" value="/dev/ttyS2" />
		<param name="baud_rate" value="38400" />
		<param name="rate" value="30" />
		<param name="base_width" value="0.314" />
		<param name="ticks_per_metre" value="21738" />
		<param name="base_frame_id" value="base_link" />
		<param name="QPPS" value="11600" />
   </node>
</launch>

